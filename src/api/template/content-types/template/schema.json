{
  "kind": "collectionType",
  "collectionName": "templates",
  "info": {
    "singularName": "template",
    "pluralName": "templates",
    "displayName": "Template"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "template_name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "maxLength": 100,
      "minLength": 5,
      "required": true
    },
    "template_text": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "maxLength": 255,
      "required": true
    },
    "experts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::expert.expert",
      "mappedBy": "templates"
    }
  }
}
